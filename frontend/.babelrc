{
  "presets": ["next/babel"],
  "env": {
    "test": {
      "plugins": ["babel-plugin-istanbul"],
      "only": ["src/components/**/*", "src/app/**/*"]
    }
  },
  "ignore": [
    "node_modules",
    "node_modules/**/*",
    ".next",
    "out",
    ".git",
    "src/middleware.tsx",
    "src/openapi/*",
    "src/openapi/**/*",
    "**/chunk-*.js"
  ],
  "compact": true
}
